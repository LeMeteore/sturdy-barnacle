def chebyshev_distance(p1, p2):
    """
    Calcule la distance de Chebyshev entre deux points p1 et p2.

    Args:
        p1 (tuple/list): Le premier point comme une liste de coordonnées.
        p2 (tuple/list): Le second point comme une liste de coordonnées.

    Returns:
        float: La distance de Chebyshev entre les deux points.
    """
    if len(p1) != len(p2):
        raise ValueError("Les points doivent avoir la même dimension")

    max_diff = 0
    for i in range(len(p1)):
        diff = abs(p1[i] - p2[i])
        max_diff = max(max_diff, diff)

    return max_diff

# Exemple d'utilisation
if __name__ == "__main__":
    point1 = (1, 2, 3)
    point2 = (4, 5, 6)
    distance = chebyshev_distance(point1, point2)
    print("Distance de Chebyshev:", distance)
